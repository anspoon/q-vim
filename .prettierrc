{
  "printWidth": 120,
  "tabWidth": 4,
  "trailingComma": "none",
  "jsxBracketSameLine": true,
  "semi": true,
  "singleQuote": true,
  "overrides": [
    {
      "files": ["*.json", ".eslintrc", ".tslintrc", ".prettierrc", ".tern-project"],
      "options": {
        "tabWidth": 2
      }
    },
    {
      "files": ["*.vue"],
      "options": {
        "tabWidth": 2,
        "parser": "vue"
      }
    }
  ]
}
